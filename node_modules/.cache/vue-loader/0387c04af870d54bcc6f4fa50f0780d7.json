rs({\n        events: 'eventsList',\n        search: 'query',\n        isSidebar: 'sidebar'\n      }),\n      searchedEvents () {\n        if(this.search) {\n          let data = this.events.filter(e => e.title.toLowerCase().match(this.search.toLowerCase()))\n          return data\n        }\n        return this.events\n      }\n    },\n    methods: {\n      ...mapActions({\n        fetchEvents: 'fetchEvents'\n      })\n    },\n  }\n</script>\n<style scoped>\n.main {\n  padding: 64px 130px 0px 130px;\n  background: #FFFFFF;\n}\n.main--header {\n  font-style: normal;\n  font-weight: 800;\n  font-size: 36px;\n  line-height: 40px;\n  text-transform: uppercase;\n  color: #212121;\n}\n.count {\n  font-weight: 300;\n  padding-left: 8px;\n}\n@media (max-width: 1280px) {\n  .main {\n    padding: 64px 32px 0px 32px;\n  }\n}\n@media  (max-width: 760px) {\n  .main {\n    padding: 40px 24px 0px 24px;\n  }\n  .main--header {\n    font-size: 24px;\n    line-height: 27px;\n  }\n}\n\n</style>"]}]}